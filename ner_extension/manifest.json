{
    "name": "Named Entity Recognizer",
    "description" : "Base Level Extension",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon48.png"
    },

    "content_scripts": [{
      "js": [
        "script/jquery.min.js",
        "script/inference.js"
      ],
      "matches": ["http://*/*", "https://*/*"],
      "run_after": "document_start"
    }]
}